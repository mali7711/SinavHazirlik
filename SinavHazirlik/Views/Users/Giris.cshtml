@model UsersModel

<form action="/Users/Listele" method="GET" >
    <div class="form-group">
        <label>KullaniciAdi</label>
        <input name="KullaniciAdi" class="form-control">
        <span asp-validation-for="KullaniciAdi" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label>Parola</label>
        <input type="password" name="Parola" class="form-control" />
        <span asp-validation-for="Parola" class="text-danger"></span>
    </div>
    <div class="form-group">
        <button style="text-align-center" type="submit" class="btn btn-success">Giriş Yap</button>
        <br />
        
        <span asp-validation-for="KullaniciAdiuyari" class="text-danger"> </span>
        <span asp-validation-for="Parolauyari" class="text-danger"> </span>

    </div>
 </form>
